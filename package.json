{
  "name": "@veaba/qrcodes-monorepos",
  "version": "0.0.1-alpha",
  "private": true,
  "license": "MIT",
  "type": "module",
  "scripts": {
    "clean": "node scripts/clean.js",
    "rsw": "rsw",
    "watch": "pnpm rsw watch",
    "build": "pnpm -r --filter './packages/*' run build",
    "build:js": "pnpm --filter @veaba/js-shared build && pnpm --filter @veaba/qrcode-js build",
    "build:node": "pnpm --filter @veaba/js-shared build && pnpm --filter @veaba/qrcode-node build",
    "build:wasm": "pnpm --filter @veaba/qrcode-wasm build",
    "build:rust": "pnpm --filter @veaba/qrcode-fast build && pnpm --filter @veaba/qrcode-rust build",
    "test": "node scripts/test.js",
    "test:unit": "vitest run",
    "test:browser": "vitest run --config vitest.config.browser.ts",
    "test:browser:ui": "HEADED=1 vitest --config vitest.config.browser.ts",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:shared": "node scripts/test.js --shared",
    "test:node": "node scripts/test.js --node",
    "test:ts": "node scripts/test.js --ts",
    "benchmark": "node bench/scripts/run.js",
    "benchmark:backend": "node bench/scripts/run.js",
    "benchmark:node": "node bench/scripts/run.js --node",
    "benchmark:bun": "node bench/scripts/run.js --bun",
    "benchmark:rust": "node bench/scripts/run.js --rust",
    "bench:svg": "node bench/svg-benchmark/index.js",
    "bench:svg:rust": "node bench/svg-benchmark/rust.js",
    "bench:svg:rust:quick": "node bench/svg-benchmark/rust.js --quick",
    "bench:svg:rust:full": "node bench/svg-benchmark/rust.js",
    "bench:svg:js": "node bench/svg-benchmark/node-bun.js",
    "bench:svg:js:node": "node bench/svg-benchmark/node-bun.js --node",
    "bench:svg:js:bun": "node bench/svg-benchmark/node-bun.js --bun",
    "bench:svg:quick": "node bench/svg-benchmark/index.js --quick",
    "bench:svg:full": "node bench/svg-benchmark/index.js --full",
    "publish:check": "node scripts/publish-check.js",
    "publish:all": "node scripts/publish.js",
    "publish:dry-run": "node scripts/publish.js --dry-run",
    "publish:help": "echo '查看 PUBLISH.md 获取详细发包指南'",
    "docs:dev": "rspress dev",
    "docs:build": "rspress build",
    "docs:preview": "rspress preview",
    "changeset": "changeset",
    "changeset:add": "changeset add",
    "changeset:status": "changeset status",
    "changeset:version": "changeset version",
    "changeset:publish": "changeset publish",
    "ci:version": "changeset version && pnpm install",
    "ci:publish": "changeset publish",
    "clippy": "cargo clippy --fix"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@types/node": "^25.1.0",
    "@vitest/browser": "^2.1.9",
    "@vitest/coverage-v8": "^2.1.0",
    "playwright": "^1.58.1",
    "rimraf": "^6.0.1",
    "typescript": "^4.8.4",
    "vite": "^5.0.0",
    "vitest": "^2.1.9"
  },
  "dependencies": {
    "rspress": "^1.47.1"
  },
  "packageManager": "pnpm@9.15.4+sha512.b2dc20e2fc72b3e18848459b37359a32064663e5627a51e4c74b2c29dd8e8e0491483c3abb40789cfd578bf362fb6ba8261b05f0387d76792ed6e23ea3b1b6a0"
}