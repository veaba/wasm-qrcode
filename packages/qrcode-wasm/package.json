{
  "name": "@veaba/qrcode-wasm",
  "version": "0.2.0",
  "description": "WebAssembly QRCode generator - Unified API with qrcode-js",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "pkg"
  ],
  "scripts": {
    "build:wasm": "wasm-pack build --target web --out-dir pkg",
    "build:ts": "tsc",
    "build": "npm run build:wasm && npm run build:ts",
    "build:node": "wasm-pack build --target nodejs --out-dir pkg",
    "build:bundler": "wasm-pack build --target bundler --out-dir pkg",
    "prepare": "npm run build"
  },
  "keywords": [
    "qrcode",
    "rust",
    "wasm",
    "webassembly",
    "qr",
    "barcode"
  ],
  "author": "veaba",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/veaba/qrcodes"
  },
  "devDependencies": {
    "typescript": "^5.0.0"
  }
}
