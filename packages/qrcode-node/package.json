    "qr",
    "barcode"
  ],
  "author": "veaba",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/veaba/qrcodes"
  },
  "dependencies": {
    "@veaba/js-shared": "workspace:*"
  },
  "devDependencies": {
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
