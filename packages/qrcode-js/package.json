{
  "name": "@veaba/qrcode-js",
  "version": "1.0.0",
  "author": "veaba",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/veaba/qrcodes"
  },
  "description": "A browser-compatible QRCode library with caching - extends @veaba/qrcode-shared",
  "type": "module",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "unpkg": "dist/index.iife.js",
  "jsdelivr": "dist/index.iife.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./browser": {
      "import": "./dist/index.iife.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsdown",
    "watch": "tsdown --watch"
  },
  "keywords": [
    "qrcode",
    "javascript",
    "browser",
    "qr",
    "barcode",
    "cache"
  ],
  "dependencies": {
    "@veaba/qrcode-shared": "workspace:*"
  },
  "devDependencies": {
    "tsdown": "^0.10.0",
    "typescript": "^5.0.0",
    "vite": "^3.2.1",
    "vitest": "^1.0.0"
  }
}
