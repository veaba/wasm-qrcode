{
  "name": "@veaba/qrcode-shared",
  "version": "0.0.1-alpha",
  "description": "Shared QRCode utilities and core logic for all @veaba/qrcode packages",
  "type": "module",
  "private": true,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./qr-core": {
      "import": "./dist/qr-core.js",
      "types": "./dist/qr-core.d.ts"
    },
    "./cache": {
      "import": "./dist/cache.js",
      "types": "./dist/cache.d.ts"
    },
    "./styles": {
      "import": "./dist/styles.js",
      "types": "./dist/styles.d.ts"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "test": "bun test"
  },
  "keywords": [
    "qrcode",
    "shared",
    "utilities",
    "core"
  ],
  "author": "veaba",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/veaba/qrcodes"
  },
  "devDependencies": {
    "typescript": "^5.0.0",
    "@types/bun": "latest"
  }
}